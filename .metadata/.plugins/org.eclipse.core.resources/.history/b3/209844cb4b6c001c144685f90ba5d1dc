package EXERCISE;

import java.util.*;

public class EXERCISE_1 {
 
	static int enterNum;
	static Scanner input = new Scanner(System.in);
	
	public static void main(String[] args) {
		
		System.out.print("Enter a number: ");
		 enterNum = input.nextInt();
		
		for(int i = 1; i < enterNum; i++) {
			System.out.println(i);
			
		}
		
		printNumAsc(enterNum);
	}
	
	public static void printNumAsc(int enterNum) {
		
		if(enterNum == 0) {
			return;
		}else {
			System.out.println(enterNum);
			printNumAsc(enterNum - 1);
		}
		
	}
}
